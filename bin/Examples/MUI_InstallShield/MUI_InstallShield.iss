; 演示如何使用 InstallShield 界面风格的 INNO 安装程序。
; 脚本： restools ( http://restools.hanzify.org )

[Setup]
WizardSmallImageFile=bottom.bmp
AppName=My Program
AppVersion=1.5
DefaultDirName={pf}\My Program
DefaultGroupName=My Program
UninstallDisplayIcon={app}\MyProg.exe
OutputDir=.
SolidCompression=yes
Compression=lzma/ultra
WizardImageFile=Left.bmp
LicenseFile=license.txt
UserInfoPage=true
InfoBeforeFile=infobefore.txt
InfoAfterFile=infoafter.txt
Password=1234
UsePreviousTasks=yes

[Tasks]
Name: task1; Description: My Task 1; GroupDescription: Task List;

[Types]
Name: "full"; Description: "Full installation"
Name: "compact"; Description: "Compact installation"
Name: "custom"; Description: "Custom installation"; Flags: iscustom

[Components]
Name: "program"; Description: "Program Files"; Types: full compact custom; Flags: fixed
Name: "help"; Description: "Help File"; Types: full
Name: "readme"; Description: "Readme File"; Types: full
Name: "readme\en"; Description: "English"; Flags: exclusive
Name: "readme\de"; Description: "German"; Flags: exclusive

[Files]
;{ ISFormDesignerFilesBegin } // Don't remove this line!
