procedure RedesignWizardForm;
begin
  { Creates custom wizard page }
  ISCustomPage1 := CreateCustomPage(wpWelcome, 'ISCustomPage1_Caption', 'ISCustomPage1_Description');

  { ISCustomPage1 }
  with ISCustomPage1.Surface do
  begin
    Name := 'ISCustomPage1';
  end;

  { Label1 }
  Label1 := TLabel.Create(WizardForm);
  with Label1 do
  begin
    Name := 'Label1';
    Parent := ISCustomPage1.Surface;
    Caption := 'Url:';
    Transparent := False;
    Left := ScaleX(16);
    Top := ScaleY(8);
    Width := ScaleX(17);
    Height := ScaleY(13);
  end;

  { Label2 }
  Label2 := TLabel.Create(WizardForm);
  with Label2 do
  begin
    Name := 'Label2';
    Parent := ISCustomPage1.Surface;
    Caption := 'FilePath';
    Transparent := False;
    Left := ScaleX(16);
    Top := ScaleY(64);
    Width := ScaleX(38);
    Height := ScaleY(13);
  end;

  { NewEdit1 }
  NewEdit1 := TNewEdit.Create(WizardForm);
  with NewEdit1 do
  begin
    Name := 'NewEdit1';
    Parent := ISCustomPage1.Surface;
    Left := ScaleX(15);
    Top := ScaleY(24);
    Width := ScaleX(385);
    Height := ScaleY(21);
    Text := '';
  end;

  { NewEdit2 }
  NewEdit2 := TNewEdit.Create(WizardForm);
  with NewEdit2 do
  begin
    Name := 'NewEdit2';
    Parent := ISCustomPage1.Surface;
    Left := ScaleX(16);
    Top := ScaleY(82);
    Width := ScaleX(384);
    Height := ScaleY(21);
    Text := '';
