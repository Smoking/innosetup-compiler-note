  { NewButton1 }
  NewButton1 := TNewButton.Create(WizardForm);
  with NewButton1 do
  begin
    Name := 'NewButton1';
    Parent := ISCustomPage1.Surface;
    Left := ScaleX(72);
    Top := ScaleY(144);
    Width := ScaleX(249);
    Height := ScaleY(41);
    Caption := 'GetFile';
    OnClick := @NewButton1Click;
  end;

  NewEdit1.TabOrder := 0;
  NewEdit2.TabOrder := 1;
  NewButton1.TabOrder := 2;

{ ReservationBegin }
  NewEdit1.Text := 'http://www.sherlocksoftware.org/innotools/example3%202.0.exe';
